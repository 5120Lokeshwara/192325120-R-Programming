num_vector <- c(1, 2, 3, 4, 5, 6)   # Numeric
comp_vector <- c(1+2i, 2+3i, 3+4i, 4+5i, 5+6i, 6+7i)  # Complex
log_vector <- c(TRUE, FALSE, TRUE, FALSE, TRUE, FALSE)  # Logical
char_vector <- c("A", "B", "C", "D", "E", "F")  # Character

cat("Numeric vector:\n", num_vector, "\n")
cat("Complex vector:\n", comp_vector, "\n")
cat("Logical vector:\n", log_vector, "\n")
cat("Character vector:\n", char_vector, "\n")
